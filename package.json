{
  "name": "weialbert.github.io",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run build:resume && astro dev",
    "build": "npm run build:resume && astro build",
    "build:resume": "tsx src/lib/resume/build.ts --all",
    "build:resume:default": "tsx src/lib/resume/build.ts --profile=default",
    "build:resume:onepage": "tsx src/lib/resume/build.ts --profile=onepage",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rm -rf public/resume"
  },
  "dependencies": {
    "astro": "^5.16.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}